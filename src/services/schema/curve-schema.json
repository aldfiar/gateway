{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "allowedSlippage": {
      "type": "string"
    },
    "gasLimitEstimate": {
      "type": "integer"
    },
    "ttl": {
      "type": "integer"
    },
    "secondary": {
      "type": "object",
      "properties": {
        "chain": {
          "type": "string"
        },
        "network": {
          "type": "string"
        }
      },
      "required": [
        "chain",
        "network"
      ]
    },
    "tokens": {
      "type": "object",
      "patternProperties": {
        "^\\w+$": {
          "type": "object",
          "properties": {
            "token": {
              "type": "string"
            }
          },
          "required": [
            "token"
          ],
          "additionalProperties": false
        }
      }
    },
    "contractAddresses": {
      "type": "object",
      "patternProperties": {
        "^\\w+$": {
          "type": "object",
          "properties": {
            "routerAddress": {
              "type": "string"
            }
          },
          "required": [
            "routerAddress"
          ],
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": true,
  "required": [
    "allowedSlippage",
    "gasLimitEstimate",
    "ttl",
    "contractAddresses",
    "secondary"
  ]
}
